#!/home/daniele/django_proxy_swift/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pbr==1.10.0','console_scripts','pbr'
__requires__ = 'pbr==1.10.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pbr==1.10.0', 'console_scripts', 'pbr')()
    )
